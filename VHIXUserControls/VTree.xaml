<UserControl
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:VHIXUserControls"
             xmlns:syncfusion="http://schemas.syncfusion.com/wpf" x:Class="VTree"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>

        <syncfusion:TreeViewAdv HorizontalAlignment="Left" Height="450" Margin="10,10,0,0" VerticalAlignment="Top" Width="200">
            <syncfusion:TreeViewItemAdv EditedItemTemplate="{x:Null}" EditedItemTemplateSelector="{x:Null}" ExpanderStyle="{x:Null}" FakeItemTemplate="{x:Null}" FakeItemTemplateSelector="{x:Null}" Header="New TreeViewItem" IsImageSourceFreeze="False"/>
        </syncfusion:TreeViewAdv>

    </Grid>
</UserControl>
